{
  "tokenTypes": [ 
    {
      "name": "heading",
      "regex": "(^|\\n)#"
    },
    {
      "name": "link",
      "regex": "\\[.*?\\]\\(.*?\\)"
    },
    {
      "name": "codeBlock",
      "regex": "```"
    },
    {
      "name": "codeInline",
      "regex": "`"
    }
  ],
  "tokens": {
    "heading": {
      "breakOn": "\\n",
      "className": "docComment"
    },
    "link": {
      "breakAfter": "\\[.*?\\]\\(.*?\\)",
      "className": "string"
    },
    "codeBlock": {
      "breakAfter": "[^`]```",
      "className": "number"
    },
    "codeInline": {
      "breakAfter": "[^`]`",
      "className": "number"
    }
  }
}
