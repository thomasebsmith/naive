{
  "tokenTypes": [ 
    {
      "name": "heading",
      "regex": "(^|\\n)#"
    },
    {
      "name": "link",
      "regex": "\\[.*?\\]\\(.*?\\)"
    },
    {
      "name": "codeBlock",
      "regex": "```"
    },
    {
      "name": "codeInline",
      "regex": "`"
    },
    {
      "name": "listBullet",
      "regex": "(^|\\n)\\s*?-"
    },
    {
      "name": "bold",
      "regex": "\\*\\*.+?\\*\\*"
    },
    {
      "name": "italic",
      "regex": "(\\*|_).+?\\1"
    }
  ],
  "tokens": {
    "heading": {
      "breakOn": "\\n",
      "className": "docComment"
    },
    "link": {
      "breakAfter": "\\[.*?\\]\\(.*?\\)",
      "className": "string"
    },
    "codeBlock": {
      "breakAfter": "[^`]```",
      "className": "number"
    },
    "codeInline": {
      "breakAfter": "[^`]`",
      "className": "number"
    },
    "listBullet": {
      "breakAfter": "-",
      "className": "operator"
    },
    "bold": {
      "breakAfter": "\\*\\*.+?\\*\\*",
      "className": "keyword"
    },
    "italic": {
      "breakAfter": "(\\*|_).+?\\1",
      "className": "identifier"
    }
  }
}
